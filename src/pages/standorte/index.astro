---
import BaseLayout from '@/layouts/base-layout.astro';
import { FinalCTA } from '@/components/sections';
import { SITE_URL } from '@/config/site';

const title = 'Standorte';
const description = 'Unsere Standorte in Ihrer Nähe. Wir sind deutschlandweit für Sie da.';
const canonicalUrl = `${SITE_URL}/standorte/`;
---

<BaseLayout title={title} description={description} canonicalUrl={canonicalUrl}>
  <main>
    <FinalCTA />
  </main>
</BaseLayout>
